{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "img/requrl.png",
      "default_title": "HTTP Headers"
   },
   "description": "Monitor HTTP/HTTPs traffic from your browser.",
     "manifest_version": 2,
     "name": "HTTP Headers",
     "offline_enabled": true,
     "permissions": [ "webRequest", "webRequestBlocking", "tabs", "http://*/", "https://*/", "\u003Call_urls>" ],
   "version": "1.1"
}
